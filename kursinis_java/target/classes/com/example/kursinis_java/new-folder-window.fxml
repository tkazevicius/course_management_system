      <ComboBox fx:id="responsiblePerson" layoutX="86.0" layoutY="104.0" prefWidth="150.0" />
      <Text fx:id="responsibleTextField" layoutX="14.0" layoutY="121.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Responsible:" />
      <Button fx:id="createFolderButton" layoutX="42.0" layoutY="166.0" mnemonicParsing="false" onAction="#createFolder" text="Create" />
      <Button layoutX="177.0" layoutY="166.0" mnemonicParsing="false" onAction="#returnToMain" text="Cancel" />
      <Button fx:id="updateFolderButton" layoutX="39.0" layoutY="166.0" mnemonicParsing="false" onAction="#updateFolder" text="Update" />
